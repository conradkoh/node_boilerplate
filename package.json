    "dependencies": {
        "apidoc": "^0.22.1",
        "chai": "^4.2.0",
        "dotenv": "^8.2.0",
        "esm": "^3.2.25",
        "express": "^4.17.1",
        "mocha": "^7.2.0",
        "mysql2": "^2.1.0",
        "nodemon": "^2.0.4",
        "sequelize": "^5.21.10",
        "sequelize-cli": "^5.5.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.6",
        "@types/node": "^14.0.5",
        "@types/sequelize": "^4.28.9"
    }
